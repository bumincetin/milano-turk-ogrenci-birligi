{
  "kind": "collectionType",
  "collectionName": "ticket_logs",
  "info": {
    "singularName": "ticket-log",
    "pluralName": "ticket-logs",
    "displayName": "Ticket Log",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "datetime"
    },
    "purchase": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::purchase.purchase"
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket"
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "transfer_from": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "redemption": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "payment": {
      "type": "enumeration",
      "enum": [
        "bank-transfer",
        "credit-card"
      ]
    }
  }
}
