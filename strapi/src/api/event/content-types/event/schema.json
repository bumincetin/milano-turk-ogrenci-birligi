{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "event_time": {
      "type": "datetime",
      "required": false
    },
    "last_enroll_time": {
      "type": "datetime",
      "required": false
    },
    "summary": {
      "type": "text"
    },
    "details": {
      "type": "blocks"
    },
    "blocked": {
      "type": "boolean"
    },
    "person_limit": {
      "type": "integer",
      "min": 0
    },
    "current_person_count": {
      "type": "integer",
      "min": 0
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "City Tour",
        "Workshop",
        "Cultural",
        "Food",
        "Sport",
        "Meeting"
      ]
    },
    "location": {
      "type": "string"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    }
  }
}
