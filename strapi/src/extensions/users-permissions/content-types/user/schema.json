{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "birthday": {
      "type": "date"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "company_email": {
      "type": "email"
    },
    "position": {
      "type": "string"
    },
    "twitter": {
      "type": "text"
    },
    "linkedin": {
      "type": "text"
    },
    "website": {
      "type": "text"
    },
    "billing_information": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::billing-information.billing-information",
      "inversedBy": "user"
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase.purchase",
      "mappedBy": "user"
    },
    "ticket": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ticket.ticket",
      "mappedBy": "redemption_user"
    },
    "telephone": {
      "type": "string",
      "regex": "^(\\+90|0)?\\s*(\\(\\d{3}\\)[\\s-]*\\d{3}[\\s-]*\\d{2}[\\s-]*\\d{2}|\\(\\d{3}\\)[\\s-]*\\d{3}[\\s-]*\\d{4}|\\(\\d{3}\\)[\\s-]*\\d{7}|\\d{3}[\\s-]*\\d{3}[\\s-]*\\d{4}|\\d{3}[\\s-]*\\d{3}[\\s-]*\\d{2}[\\s-]*\\d{2})$",
      "maxLength": 14
    },
    "universityName": {
      "type": "string",
      "required": true
    },
    "universityDepartment": {
      "type": "string",
      "required": true
    },
    "universityClass": {
      "type": "string",
      "required": true
    },
    "blog_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "users_permissions_user"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "users"
    }
  }
}
